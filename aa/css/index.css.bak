body {margin-left:auto;margin-right:auto;font-size:13px;line-height:23px;font-family:Microsoft YaHei,'微软雅黑';}
a {color:#000;text-decoration:none;}
a:hover {text-decoration:underline;}
img {border:0px;float:left;}
* {margin:0px;padding:0px;list-style-type:none;}


.main{
	width:100%;
	max-width:640px;
	min-width:320px;
	height:auto;
	margin:0 auto;
	overflow:hidden;
	position:relative;
	margin-bottom:0.86rem;
}
.main img{
	width:100%;
	float:left;
}
/* star */
.star{
	width:100%;
	float:left;
}
.star li{
	width:50%;
	height:auto;
	float:left;
}
.star li img{
	width:100%;
	float:left;
}

/* shop */
.order{
	width:100%;
	height:auto;
	min-height:7rem;
}
/* foot */
.fixed{
	width:100%;
	max-width:640px;
	min-width:320px;
	height:0.86rem;
	position:fixed;
	bottom:0;
}
.fixed li{
	width:1.28rem;
	height:0.86rem;
	float:left;
}
.fixed li img{
	width:100%;
	float:left;
}